Resources:
  ## Custom ELB configs for load balancer
  elb:
    Type: AWS::ElasticLoadBalancing::LoadBalancer
    Properties:
      Subnets: '`{ "Ref" : "ELBSubnets" }`' 
#        Fn::GetOptionSetting: {OptionName: ELBSubnets}
      Listeners:
      - LoadBalancerPort: '80'
        InstancePort: '8080'
        Protocol: HTTP 
